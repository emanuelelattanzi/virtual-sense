<project basedir="../../../../">

	<import file="../../vm.build.xml" />

	<!-- the name of this distro -->
	<property name="distro" value="virtualsense" />

	<!-- the cpu and platform -->
	<property name="arm" value="arm" />
	<property name="platform" value="contiki" />

	<!-- Include stuff from opt -->
	<patternset id="opt">
		<include name="darjeeling/contiki/**/*.*" />
		<include name="base/contiki/**/*.*" />
		<include name="virtualsense/contiki/**/*.*" />
	</patternset>

	<import file="../../vm.contiki.arm.build.xml" />

</project>
